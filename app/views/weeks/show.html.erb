<nav>
  <div class="mobile-content">
    <i class="icon-left-open"></i>
    <h2><%= @week.number %>. týždeň</h2>
    <i style="float: right; " class="icon-right-open"></i>
    <i style="float: right; " class="icon-calendar-empty"></i>
  </div>
</nav>
<hr class="nav-offset">
<div class="nav-info mobile-content">
    12.3. - 19.3.2015
    <div class="week-progressbar"></div>
    <div class="caption">Dokončené 3/10 (30%)</div>
</div>

<div class="mobile-content main">
  <% @learning_objects.each do |lo| %>
      <div class="question-card">
        <div class="question-card-content">
            <h3><%= lo.lo_id %></h3>
            <p>
                <%= lo.question_text %>
            </p>
        </div>
        <div class="question-card-info">
          <i class="icon-eye"></i> Videné (3x) <i class="icon-ok"></i> Vyriešené
        </div>
      </div>
  <% end %>
</div>
<script>
    $(function() {
        $( ".week-progressbar" ).progressbar({
            value: 60
        });
    });
</script>