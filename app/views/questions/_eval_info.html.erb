
<div class="nav-info mobile-content">
    <% if !(current_user.show_solutions) %>
      <% if @room.state.to_s != "used" %>
          <%if (@room.score < @room.score_limit)%>
            <%= "Dosiahnuté skóre:"%>
          <%else%>
            <%= "Gratulujeme, dosiahli ste minimálne skóre!"%>
          <%end%>
      <%else%>
          <%= "Tento test ste už úspešne vyriešili." %>
      <%end%>
      <div class="question-progressbar" data-number-all="<%= @room.score_limit %>" data-number-done="<%= @room.score %>">
        <div class="progressbar-caption"></div>
      </div>
    <%end%>
  <%= @number_of_question.to_s + ". otázka z " + @room.learning_objects.count.to_s %>
</div>
