    <%= link_to "+ #{t('admin.questions.links.create')}", new_admin_learning_object_path(course: @course), class: 'btn' %>
  </div>

  <div class="tools">
  </div>

</div>

<div id="questions-list" class="admin">
  <%= render partial: 'learning_object', collection: @questions, as: :learning_object %>
